<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8"/>

    <!-- Specific for mobile device -->
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1"/>
    <link rel="apple-touch-icon" href="static/img/deejayd.jpg"/>

    <!-- Page title                         -->
    <title>Deejayd Mobile Webui</title>

    <!--                                           -->
    <!--    Load style                             -->
    <!--                                           -->
    <link rel="stylesheet"
          href="static/style/jquery.mobile-1.3.1.min.css"/>
    <link rel="stylesheet"
          href="static/style/deejayd.css"/>

    <!--                                           -->
    <!--    Load library scripts                   -->
    <!--                                           -->
    <script type="text/javascript" language="javascript"
            src="static/js/lib/sockjs-0.3.min.js"></script>
    <script type="text/javascript" language="javascript"
            src="static/js/lib/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" language="javascript"
            src="static/js/lib/jquery.mobile-1.3.1.min.js"></script>
    <script type="text/javascript" language="javascript"
            src="static/js/lib/jquery.i18n.min.js"></script>

    <!--                                           -->
    <!--    Load namespace var and options         -->
    <!--                                           -->
    <script>
        window.DjdApp = {
            models: {},
            views: {},
            widgets: {},
            options: {
                root_url: '%(root_url)s'
            },
            dictionary: '%(i18n_dict)s'
        };
    </script>
    %(custom_scripts)s

</head>

<body>
    <div id="djd-player-page" data-role="page">
        <div data-role="panel" id="djd-player-panel"
             data-display="push" data-theme="a">
            <ul data-role="listview">
                <li>
                    <a href="#djd-audiolib-page">
                        %(music)s
                    </a>
                </li>
                <li>
                    <a href="#djd-videolib-page">
                        %(video)s
                    </a>
                </li>
            </ul>
        </div><!-- /panel -->

        <div data-role="header" data-position="fixed">
            <h1 id="djd-player-page-title"></h1>
            <a href="#djd-player-panel" data-icon="home" data-role="button"
               data-iconpos="notext"></a>
            <a href="#djd-playlist-page" data-icon="bars" data-role="button"
               data-iconpos="notext" data-transition="flip"></a>
        </div><!-- /header -->

        <div data-role="content" id="djd-player-content">
        </div><!-- /content -->

        <div data-role="footer" id="djd-player-footer" data-position="fixed">
            <div class="ui-grid-solo">
                <input type="range" name="djd-player-slider" id="djd-player-slider"
                       min="0" max="100" value="0"
                       data-highlight="true"></input>
            </div>
        </div><!-- /footer -->

        <div id="djd-player-popup" data-role="popup" data-dismissible="true">
           <div data-role="content" id="djd-player-popup-content"></div>
        </div>
    </div><!-- player page -->

    <div id="djd-playlist-page" data-role="page">
        <div data-role="header" data-position="fixed">
            <h1 id="djd-playlist-title"></h1>
            <a href="#djd-player-page" data-icon="delete"
               data-role="button" data-transition="flip">
                Close
            </a>
            <div id="djd-playlist-toolbar">
                <div style="float:left" data-role="controlgroup"
                     data-type="horizontal"
                     id="djd-playlist-toolbar-buttons">
                </div>
                <div style="float:right">
                    <fieldset data-role="controlgroup" data-type="horizontal">
                        <input type="checkbox" id="djd-playlist-toolbar-rpt"
                               name="djd-playlist-toolbar-rpt"
                               data-mini="true"/>
                        <label for="djd-playlist-toolbar-rpt">%(repeat)s</label>

                        <select name="djd-playlist-toolbar-order"
                                id="djd-playlist-toolbar-order"
                                data-mini="true">
                        </select>
                    </fieldset>
                </div>
            </div>
        </div><!-- /header -->

        <div data-role="content" id="djd-playlist-content">
            <ul data-role="listview" id="djd-playlist-listview"></ul>
        </div><!-- /content -->

        <div data-role="footer" id="djd-playlist-footer" data-position="fixed">
            <div data-role="navbar" data-theme="b">
                <ul>
                    <li><a href="#"
                           class="ui-btn-active ui-state-persist"
                           data-djd-source="audiopls"
                           id="djd-playlist-nav-audiopls">Audio playlist</a>
                    </li>
                    <li><a href="#"
                           data-djd-source="videopls"
                           id="djd-playlist-nav-videopls">Video playlist</a>
                    </li>
                    <li><a href="#"
                           data-djd-source="audioqueue"
                           id="djd-playlist-nav-audioqueue">Audio Queue</a>
                    </li>
                </ul>
            </div><!-- /navbar -->
        </div><!-- /footer -->
    </div><!-- playlist page -->

    <div id="djd-audiolib-page" data-role="page">
        <div data-role="panel" id="djd-audiolib-panel"
             data-display="push" data-theme="a">
            <ul data-role="listview">
                <li>
                    <a href="#djd-player-page">
                        Now Playing
                    </a>
                </li>
                <li>
                    <a href="#djd-videolib-page">
                        %(video)s
                    </a>
                </li>
            </ul>
        </div><!-- /panel -->

        <div data-role="header" data-position="fixed">
            <h1 id="djd-audiolib-page-title">%(music)s</h1>
            <a href="#djd-audiolib-panel" data-icon="home" data-role="button"
               data-iconpos="notext"></a>
            <div class="ui-body-c" id="djd-audiolib-toolbar">
                <ul class="djd-inline-block" id="djd-audiolib-toolbar-buttons">
                </ul>
                <div style="display: inline-block;">
                    <h2 id="djd-audiolib-toolbar-title"></h2>
                </div>
            </div>
        </div><!-- /header -->

        <div data-role="content" id="djd-audiolib-content">
            <ul data-role="listview" id="djd-audiolib-listview"
                data-split-icon="star"></ul>
        </div><!-- /content -->

        <div data-theme="a" data-role="footer" id="djd-audiolib-footer"
             data-position="fixed">
            <div data-role="navbar" data-theme="b" id="djd-audiolib-nav">
                <ul>
                    <li><a href="#"
                           class="ui-btn-active ui-state-persist"
                           id="djd-audiolib-nav-genre">%(genre)s</a>
                    </li>
                    <li>
                        <a href="#" id="djd-audiolib-nav-artist">
                            %(artist)s
                        </a>
                    </li>
                    <li>
                        <a href="#" id="djd-audiolib-nav-album">
                            %(album)s
                        </a>
                    </li>
                    <li>
                        <a href="#" id="djd-audiolib-nav-folder">
                            %(folder)s
                        </a>
                    </li>
                </ul>
            </div><!-- /navbar -->
        </div><!-- /footer -->

        <div data-theme="a" id="djd-audiolib-popup" data-role="popup"
             data-dismissible="false">
            <a href="#" data-rel="back" data-role="button" data-theme="a"
               data-icon="delete" data-iconpos="notext" class="ui-btn-right"></a>

            <div data-role="header"><h1>Actions</h1></div>
            <div data-role="content">
                <ul data-role="listview" id="djd-audiolib-popup-menu">
                </ul>
            </div>
        </div>
    </div><!-- audiolib page -->

    <div id="djd-videolib-page" data-role="page">
        <div data-role="panel" id="djd-videolib-panel"
             data-display="push" data-theme="a">
            <ul data-role="listview">
                <li>
                    <a href="#djd-player-page">
                        Now Playing
                    </a>
                </li>
                <li>
                    <a href="#djd-audiolib-page">
                        Music
                    </a>
                </li>
            </ul>
        </div><!-- /panel -->

        <div data-role="header" data-position="fixed">
            <h1 id="djd-videolib-page-title">%(video)s</h1>
            <a href="#djd-videolib-panel" data-icon="home" data-role="button"
               data-iconpos="notext"></a>
            <div class="ui-body-c" id="djd-videolib-toolbar">
                <ul class="djd-inline-block" id="djd-videolib-toolbar-buttons">
                </ul>
                <div style="display: inline-block;">
                    <h2 id="djd-videolib-toolbar-title"></h2>
                </div>
            </div>
        </div><!-- /header -->

        <div data-role="content" id="djd-videolib-content">
            <ul data-role="listview" id="djd-videolib-listview"
                data-split-icon="star"></ul>
        </div><!-- /content -->

        <div data-theme="a" id="djd-videolib-popup" data-role="popup"
             data-dismissible="false">
            <a href="#" data-rel="back" data-role="button" data-theme="a"
               data-icon="delete" data-iconpos="notext" class="ui-btn-right"></a>

            <div data-role="header"><h1>Actions</h1></div>
            <div data-role="content">
                <ul data-role="listview" id="djd-videolib-popup-menu">
                </ul>
            </div>
        </div>
    </div><!-- audiolib page -->
</body>

</html>
