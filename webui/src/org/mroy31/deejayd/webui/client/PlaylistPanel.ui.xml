<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:webui="urn:import:org.mroy31.deejayd.webui.widgets"
  xmlns:cellview="urn:import:org.mroy31.deejayd.webui.cellview">
  <ui:with field="resources"
        type="org.mroy31.deejayd.webui.resources.WebuiResources"/>

     <g:DockLayoutPanel>
         <g:center>
      <g:TabLayoutPanel barHeight="24" ui:field="tabPanel">
         <g:tab>
             <g:customHeader size='24'>
                 <g:Label ui:field="dirHeader"></g:Label>
             </g:customHeader>
             <cellview:AudioLibView ui:field="audioLibView">
             </cellview:AudioLibView>
         </g:tab>

         <g:tab>
             <g:customHeader size='24'>
                   <g:Label ui:field="searchHeader"></g:Label>
             </g:customHeader>
             <cellview:AudioSearchView ui:field="audioSearchView">
             </cellview:AudioSearchView>
         </g:tab>

         <g:tab>
            <g:customHeader size='24'>
                <g:Label ui:field="plsHeader"></g:Label>
            </g:customHeader>
            <cellview:PlsListView ui:field="plsListView">
            </cellview:PlsListView>
        </g:tab>
      </g:TabLayoutPanel>
       </g:center>

       <g:south size="22">
         <g:FlowPanel width="100%">
            <webui:LibraryManager height="100%" width="100%"
              ui:field="updateButton">
            </webui:LibraryManager>
         </g:FlowPanel>
       </g:south>
    </g:DockLayoutPanel>
</ui:UiBinder>