<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources"
        type="org.mroy31.deejayd.webui.resources.WebuiResources"/>
	<ui:style>
		
	</ui:style>
	<g:FlowPanel>
	<g:DialogBox glassEnabled="true" animationEnabled="true" modal="true" ui:field="dialog">
	  <g:caption><b>New Webradio</b></g:caption>
	  
      <g:VerticalPanel width="100%" ui:field="formPanel">
         <g:HorizontalPanel spacing="10">
             <g:Label ui:field="nameLabel"></g:Label>
             <g:TextBox ui:field="nameInput"></g:TextBox>
         </g:HorizontalPanel>
         <g:HorizontalPanel spacing="10">
             <g:Label ui:field="urlLabel"></g:Label>
             <g:TextBox ui:field="urlInput"></g:TextBox>
         </g:HorizontalPanel>
         <g:HorizontalPanel spacing="10">
             <g:Label ui:field="catLabel"></g:Label>
             <g:ListBox ui:field="catInput"></g:ListBox>
         </g:HorizontalPanel>
         <g:HorizontalPanel width="100%" spacing="0">
           <g:cell horizontalAlignment="ALIGN_RIGHT">
             <g:HorizontalPanel>
               <g:Button ui:field="cancelButton"></g:Button>
	           <g:Button ui:field="addButton"></g:Button>
	         </g:HorizontalPanel>
           </g:cell>
         </g:HorizontalPanel>
      </g:VerticalPanel>
    </g:DialogBox>
    </g:FlowPanel>
</ui:UiBinder> 