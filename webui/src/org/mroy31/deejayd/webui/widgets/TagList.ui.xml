<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="resources"
        type="org.mroy31.deejayd.webui.resources.WebuiResources"/>

  <g:DockLayoutPanel>
    <g:north size="22">
      <g:FlowPanel width="100%" height="100%"
            styleName="{resources.webuiCss.toolbar}">
        <g:Label width="100%" height="100%"
                styleName="{resources.webuiCss.italic}"
                ui:field="title"></g:Label>
      </g:FlowPanel>
    </g:north>

    <g:center>
        <g:ScrollPanel ui:field="scrollList">
          <g:VerticalPanel width="100%" ui:field="tagList">
          </g:VerticalPanel>
        </g:ScrollPanel>
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder>