<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="resources"
        type="org.mroy31.deejayd.webui.resources.WebuiResources"/>

  <g:HorizontalPanel>
    <g:cell verticalAlignment="ALIGN_MIDDLE">
      <g:Button ui:field="goFirst"
            addStyleNames="{resources.webuiCss.iconOnlyButton}
                     {resources.webuiCss.goFirst}"></g:Button>
    </g:cell>
    <g:cell verticalAlignment="ALIGN_MIDDLE">
      <g:Button ui:field="goPrevious"
          addStyleNames="{resources.webuiCss.iconOnlyButton}
                     {resources.webuiCss.goPrevious}"></g:Button>
    </g:cell>
    <g:cell verticalAlignment="ALIGN_MIDDLE">
      <g:Label ui:field="pageDesc"
               addStyleNames="{resources.webuiCss.pagerDesc}"></g:Label>
    </g:cell>
    <g:cell verticalAlignment="ALIGN_MIDDLE">
      <g:Button ui:field="goNext"
          addStyleNames="{resources.webuiCss.iconOnlyButton}
                       {resources.webuiCss.goNext}"></g:Button>
    </g:cell>
    <g:cell verticalAlignment="ALIGN_MIDDLE">
      <g:Button ui:field="goLast"
          addStyleNames="{resources.webuiCss.iconOnlyButton}
                       {resources.webuiCss.goLast}"></g:Button>
    </g:cell>
  </g:HorizontalPanel>
</ui:UiBinder>