<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:webui="urn:import:org.mroy31.deejayd.webui.widgets">
  <ui:with field="resources"
        type="org.mroy31.deejayd.webui.resources.WebuiResources"/>

  <g:DockLayoutPanel>
       <g:north size="24">
           <g:HorizontalPanel
                styleName="{resources.webuiCss.modeTopToolbar}"
                ui:field="modeTopToolbar" width="100%" height="24px">
           </g:HorizontalPanel>
       </g:north>
       <g:center>
           <webui:MediaList ui:field="mediaList"></webui:MediaList>
       </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder>